<h2>New User</h2>
<%= form_for(@user) do |f| %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email%>
  </p>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </p>

  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :password %><br />
    <%= f.text_field :password %>
  </p>
  <%= f.submit %>
<% end %>

